{
  "name": "gotchi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "web": "npx webpack-dev-server -d --config ./web/webpack.config.dev.js --inline --hot --colors",
    "web:release": "npx webpack -p --config ./web/webpack.config.prod.js && cp index.html dist/index.html",
    "test": "jest"
  },
  "dependencies": {
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-native": "^0.55.22",
    "native-base": "^2.6.1",
    "react": "^16.4.1",
    "react-art": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-native": "^0.57.7",
    "react-native-progress": "^3.5.0",
    "react-native-web": "^0.8.4",
    "react-navigation": "^2.5.5"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/react-test-renderer": "^16.0.1",
    "awesome-typescript-loader": "^5.1.1",
    "babel-jest": "23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-native-web": "^0.8.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-native": "^4.0.0",
    "jest": "23.1.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
